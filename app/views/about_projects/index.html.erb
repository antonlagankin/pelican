<h1>Listing about_projects</h1>

<table>
  <thead>
    <tr>
      <th>Decription</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @about_projects.each do |about_project| %>
      <tr>
        <td><%= about_project.decription %></td>
        <td><%= link_to 'Show', about_project %></td>
        <td><%= link_to 'Edit', edit_about_project_path(about_project) %></td>
        <td><%= link_to 'Destroy', about_project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New About project', new_about_project_path %>
