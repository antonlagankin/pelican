<h1>Listing project_news</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Content</th>
      <th>Tags</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_news.each do |project_news| %>
      <tr>
        <td><%= project_news.date %></td>
        <td><%= project_news.content %></td>
        <td><%= project_news.tags %></td>
        <td><%= link_to 'Show', project_news %></td>
        <td><%= link_to 'Edit', edit_project_news_path(project_news) %></td>
        <td><%= link_to 'Destroy', project_news, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project news', new_project_news_path %>
